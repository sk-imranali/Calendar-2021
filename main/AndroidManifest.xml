        <activity android:name=".Splash_screen">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".mar" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".fev" />
        <activity android:name=".sep" />
        <activity android:name=".oct" />
        <activity android:name=".nov" />
        <activity android:name=".may" />
        <activity android:name=".jun" />
        <activity android:name=".julay" />
        <activity android:name=".dec" />
        <activity android:name=".aug" />
        <activity android:name=".April" />
        <activity android:name=".MainActivity"/>

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-7297751488937323~3320216530"/>

